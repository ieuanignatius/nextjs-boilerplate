{
  "name": "nextjs-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env node server.js",
    "build": "next build",
    "start": "cross-env NODE_ENV=production node server.js",
    "test": "jest",
    "list:check-types": "tsc --pretty --noEmit",
    "list:check": "prettier --check .",
    "lint:ts": "eslint . --ext ts,tsx --fix",
    "list:format": "prettier --write . ",
    "lint:style": "stylelint {pages,styles,components/*}/*.{css,scss,sass} --fix",
    "test:pre-commit": "npm run lint:ts && npm run lint:style && npm run test && npm run build"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "next": "canary",
    "next-pwa": "^5.2.21",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "^1.34.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.31.2",
    "@testing-library/jest-dom": "^5.13.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.1",
    "@types/react": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "babel-jest": "^27.0.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.4",
    "prettier": "^2.3.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "typescript": "^4.3.2"
  }
}
